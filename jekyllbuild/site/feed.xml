            &lt;img src=&quot;/media/compressed/gallery-3.jpg&quot; alt=&quot;Blossom in a Star&quot; /&gt;
        
            &lt;b&gt;Blossom&lt;/b&gt;
            &lt;img src=&quot;/media/compressed/gallery-4.jpg&quot; alt=&quot;Blossom&quot; /&gt;
        
            &lt;b&gt;Bubbly Bloom&lt;/b&gt;
            &lt;img src=&quot;/media/compressed/gallery-5.jpg&quot; alt=&quot;Bubbly Bloom&quot; /&gt;
        
            &lt;b&gt;Rays of Gold&lt;/b&gt;
            &lt;img src=&quot;/media/compressed/gallery-6.jpg&quot; alt=&quot;Rays of Gold&quot; /&gt;
        
            &lt;b&gt;Exotic&lt;/b&gt;
            &lt;img src=&quot;/media/compressed/gallery-7.jpg&quot; alt=&quot;Exotic&quot; /&gt;
        
            &lt;b&gt;Filled out&lt;/b&gt;
